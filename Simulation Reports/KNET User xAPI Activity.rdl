          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="object_id">
          <DataField>object_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="object_type">
          <DataField>object_type</DataField>

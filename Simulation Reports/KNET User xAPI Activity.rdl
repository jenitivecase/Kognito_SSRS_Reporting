  <DataSets>
    <DataSet Name="user_stmts">
      <Query>
        <DataSourceName>ODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@user_id">
            <Value>=Parameters!user_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
timestamp
,actor_name knet_user_id
,verb_id
,object_id
,object_type
,object_description
from lrs.dbo.statements_search 
where actor_name = @user_id
and timestamp &gt;= @start_date
and timestamp &lt;= @end_date
and (verb_id like 'course%' or verb_id like 'topic%' or verb_id like 'module%' or verb_id like 'conversation%')
order by timestamp</CommandText>
      </Query>
      <Fields>
        <Field Name="timestamp">
          <DataField>timestamp</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="knet_user_id">
